{
  "name": "with-redux-persistor",
  "displayName": "With redux persistor",
  "version": "0.0.0",
  "description": "A basic Plasmo extension.",
  "author": "coldsauce",
  "packageManager": "pnpm@7.1.5",
  "scripts": {
    "dev": "plasmo dev",
    "build": "plasmo build"
  },
  "dependencies": {
    "@plasmohq/redux-persist": "6.1.0",
    "@plasmohq/storage": "workspace:*",
    "@reduxjs/toolkit": "1.8.3",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-redux": "8.0.2",
    "redux": "4.2.0",
    "redux-persist-webextension-storage": "1.0.2",
    "redux-thunk": "2.4.1"
  },
  "devDependencies": {
    "@trivago/prettier-plugin-sort-imports": "3.2.0",
    "@types/chrome": "0.0.191",
    "@types/jest": "28.1.4",
    "@types/node": "18.0.0",
    "@types/react": "18.0.14",
    "@types/react-dom": "18.0.5",
    "plasmo": "workspace:*",
    "prettier": "2.7.1",
    "typescript": "4.7.4"
  },
  "manifest": {
    "host_permissions": [
      "https://*/*"
    ],
    "permissions": [
      "tabs",
      "storage"
    ]
  }
}
